{
  "name": "pending",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "18.9",
    "yarn": "1.22"
  },
  "scripts": {
    "build": "cd ../frontend && npm run build",
    "install-client": "cd ../frontend && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "concurrently --kill-others-on-fail \"yarn run server\" \"cd ../frontend npm run start\"",
    "server": "nodemon dist/server.js",
    "postinstall": "npx tsc",
    "dev": "nodemon --exec ts-node server.ts",
    "deploy": "git add . && git commit -m 'Heroku' && git push heroku"
  },
  "author": "Sage Maxwell",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-serve-static-core": "^4.17.31",
    "@types/node": "^16.11.6",
    "nodemon": "^2.0.14",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@typegoose/typegoose": "^9.2.0",
    "@types/serve-favicon": "^2.5.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "mongoose": "^6.0.12",
    "serve-favicon": "^2.5.0",
    "ts-node": "^10.4.0"
  }
}
